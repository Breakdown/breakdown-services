{
  "name": "app-service",
  "version": "0.1.1",
  "main": "src/index.js",
  "author": "Ryan Oillataguerre",
  "license": "MIT",
  "type": "module",
  "exports": "./dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev:test": "nodemon --watch './**/*.ts' --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' src/test-server.ts",
    "dev": "nodemon --watch './**/*.ts' --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' src/index.ts",
    "test": "jest",
    "db:seed": "node --experimental-specifier-resolution=node --loader ts-node/esm ./prisma/seed/index.ts",
    "generate": "prisma generate --schema prisma/schema.prisma --generator client",
    "migrate": "prisma migrate dev",
    "migrate:soft": "prisma migrate dev --create-only",
    "migrate:prod": "prisma migrate deploy"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/connect-redis": "^0.0.20",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.6",
    "@types/jest": "^29.5.4",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.5.9",
    "@types/validator": "^13.7.12",
    "nodemon": "^2.0.20",
    "prisma": "^5.2.0",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "connect-redis": "^6.1.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.3.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "express-validator": "^6.14.3",
    "helmet": "^6.0.1",
    "long": "^5.2.3",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "sqlx-ts": "^0.9.3",
    "tempy": "^3.1.0",
    "ts-proto": "^1.156.2",
    "validator": "^13.9.0"
  },
  "engines": {
    "node": ">=18"
  },
  "prisma": {
    "seed": "node --loader ts-node/esm ./prisma/seed/index.ts"
  }
}
