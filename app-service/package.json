{
  "name": "app-service",
  "version": "0.1.1",
  "main": "src/index.js",
  "author": "Ryan Oillataguerre",
  "license": "MIT",
  "type": "module",
  "exports": "./dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "rm -rf dist/ && tsc",
    "dev:test": "nodemon --watch './**/*.ts' --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' src/test-server.ts",
    "dev": "nodemon --watch './**/*.ts' --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' src/index.ts",
    "test": "jest",
    "db:seed": "node --experimental-specifier-resolution=node --loader ts-node/esm ./prisma/seed/index.ts",
    "generate": "prisma generate --schema prisma/schema.prisma --generator client",
    "migrate": "prisma migrate dev",
    "migrate:soft": "prisma migrate dev --create-only",
    "migrate:prod": "prisma migrate deploy",
    "createIssues": "ts-node --experimental-specifier-resolution=node ./src/seed/createIssues.ts"
  },
  "devDependencies": {
    "@jest/globals": "^29.6.4",
    "@types/body-parser": "^1.19.2",
    "@types/connect-redis": "^0.0.20",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.6",
    "@types/ioredis": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.5.9",
    "@types/twilio": "^3.19.3",
    "@types/validator": "^13.7.12",
    "jest": "^29.6.4",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@prisma/client": "5.9.1",
    "@types/bcryptjs": "^2.4.2",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "bullmq": "^5.1.6",
    "connect-redis": "^6.1.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.3.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "express-validator": "^6.14.3",
    "fast-xml-parser": "^4.3.4",
    "helmet": "^6.0.1",
    "ioredis": "^5.3.1",
    "long": "^5.2.3",
    "meilisearch": "^0.37.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.26.0",
    "twilio": "^4.8.0",
    "validator": "^13.9.0"
  },
  "engines": {
    "node": ">=18"
  }
}
